<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="SwagLabs" parallel="tests" thread-count="2">
    <test name="SwagLabs_chrome" preserve-order="true">
        <parameter name="browser_name" value="chrome"/>
        <classes>
            <class name="com.org.tests.LoginTestSauceLabs"/>
        </classes>
    </test>


    <test name="SwagLabs_edge" preserve-order="true">
        <parameter name="browser_name" value="Edge"/>
        <classes>
            <class name="com.org.tests.LoginTestSauceLabs"/>
        </classes>
    </test>
</suite>

